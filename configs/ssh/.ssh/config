Include ~/.ssh/config_prenda
Include ~/.ssh/config_artsreach

Host *
	ForwardAgent yes
	Compression no
	ServerAliveInterval 0
	ServerAliveCountMax 3
	HashKnownHosts no
	UserKnownHostsFile ~/.ssh/known_hosts
	ControlMaster auto
	ControlPath /tmp/ssh_%r@%h:%p
	ControlPersist no

Host kale
	User root
	HostName kale.lan

Host gitea
	User git
	HostName kale.lan
	Port 2222
